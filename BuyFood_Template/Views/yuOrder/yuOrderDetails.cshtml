@{
    Layout = "_LayoutAd";
}
<link href="~/assets/NEW CSS/NEWCSS.css" rel="stylesheet" />
<link href="~/assets/NEW CSS/page.css" rel="stylesheet" />

<div class="border backgroundff margin10">
    <button type="button" onclick="javascript:history.back(-1);" class="btn button_btn btn-danger">返回上一步</button>
</div>

<div class="box-module">
    <div class="box-title">訂單詳細</div>
    <div class="box-content padding15" id="order_details">

        <div class="Order_Details_style">
            <div class="Numbering clearfix">
                <span class="l_f">訂單編號:<b class="colorred">3</b></span>
                <span class="r_f">付款時間：2021-03-12 16:18</span>
            </div>
        </div>
        <div class="detailed_style">

            <!--收件人信息-->

            <div class="border-line  mb15">
                <div class="title-name text-left">收件人信息</div>
                <div class="info_list clearfix padding10">
                    <div class="col-xs-12 col-md-3 content_look">
                        <label class="label_name"> 收件人姓名： </label>
                        <div class="info_data">尤智智</div>
                    </div>
                    <div class="col-xs-12 col-md-3 content_look">
                        <label class="label_name"> 收件人電話： </label>
                        <div class="info_data">123456</div>
                    </div>
                    <div class="col-xs-12 col-md-3 content_look">
                        <label class="label_name"> 收件地郵編： </label>
                        <div class="info_data">333</div>
                    </div>
                    <div class="col-xs-12 col-md-12 content_look">
                        <label class="label_name"> 收件地址： </label>
                        <div class="info_data">台北市大安區復興南路一段390號2樓</div>
                    </div>
                </div>
            </div>

            <!--訂單信息-->
            <div class="border-line mb15">
                <div class="title-name text-left">訂單明細</div>
                <div class="Info_style  clearfix">

                    <div class="product_info clearfix" id="textorderid1">
                        <a href="#" class="img_link"><img src="~/imgs/香蕉.jpg"></a>
                        <span class="jieshao">
                            <a href="#" class="name_link">我是香蕉</a>
                            <div class="clearfix color9"><span class="l_f mr15">規格：顏色：黃色</span><span>數量：2</span></div>
                            <span>價格：<b class="price"><i>$</i>254</b></span>
                            <button style="float:right" class="btn btn-Danger btn-xs" onclick="{ $('#textorderid1').empty() };"><i class="fa fa-pencil font-18">刪除</i></button>
                        </span>
                    </div>

                    <div class="product_info clearfix" id="textorderid2">
                        <a href="#" class="img_link"><img src="~/imgs/香蕉.jpg"></a>
                        <span class="jieshao">
                            <a href="#" class="name_link">我是香蕉</a>
                            <div class="clearfix color9"><span class="l_f mr15">規格：顏色：黃色</span><span>數量：6</span></div>
                            <span>價格：<b class="price"><i>$</i>889</b></span>
                            <button style="float:right;" class="btn btn-Danger btn-xs" onclick="{ $('#textorderid2').empty() };"><i class="fa fa-pencil font-18">刪除</i></button>
                        </span>
                    </div>
                </div>
            </div>
            <!--總價-->
            <div class="Total_style border-line mb15">
                <div class="info_list clearfix padding10">
                    <div class="col-xs-12 col-md-3 content_look">
                        <label class="label_name "> 支付方式： </label>
                        <div class="info_data">在線支付</div>
                    </div>
                    <div class="col-xs-12 col-md-3 content_look">
                        <label class="label_name"> 支付狀態： </label>
                        <div class="info_data">已完成</div>
                    </div>
                    <div class="col-xs-12 col-md-3 content_look">
                        <label class="label_name"> 訂單生成日期： </label>
                        <div class="info_data">2019-03-14 16:42</div>
                    </div>
                    <div class="col-xs-12 col-md-3 content_look">
                        <label class="label_name"> 發貨日期： </label>
                        <div class="info_data">2019-03-15 20:29</div>
                    </div>
                </div>
                <div class="Total_m_style plf15">
                    <span class="Total">總數：<b>4</b></span>
                    <span class="Total_price">總價：<b>1572</b>元</span>
                    <form>
                        <input type="submit" class="btn button_btn btn-danger" style="float:right" value="確認修改" />
                    </form>
                </div>
            </div>

            <!--物流信息-->
            <div class="Logistics_style clearfix border-line mb15">
                <div class="title-name text-left">訂單訊息</div>
                <!--<div class="prompt"><img src="images/meiyou.png" /><p>暫無物流信息！</p></div>-->
                <div data-mohe-type="kuaidi_new" class="g-mohe " id="mohe-kuaidi_new">
                    <div id="mohe-kuaidi_new_nucom">
                        <div class="mohe-wrap mh-wrap">
                            <div class="mh-cont mh-list-wrap mh-unfold">
                                <div class="mh-list">
                                    <ul>
                                        <li class="first">
                                            <p>2021-04-28 11:23:28</p>
                                            <p>已到達 </p>
                                            <span class="before"></span><span class="after"></span><i class="mh-icon mh-icon-new"></i>
                                        </li>
                                        <li>
                                            <p>2021-04-27 15:00:00</p>
                                            <p>已出貨</p>
                                            <span class="before"></span><span class="after"></span>
                                        </li>
                                        <li>
                                            <p>2021-04-27 08:46:00</p>
                                            <p>訂單處理中</p>
                                            <span class="before"></span><span class="after"></span>
                                        </li>
                                        <li>
                                            <p>2021-04-26 17:12:00</p>
                                            <p>訂單已成立</p>
                                            <span class="before"></span><span class="after"></span>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script>

</script>